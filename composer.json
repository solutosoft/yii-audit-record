{
  "name": "solutosoft/yii-audit-record",
  "description": "Stores the change log from ActiveRecord",
  "keywords": [
    "yii2",
    "active-record",
    "behavior",
    "audit"
  ],
  "homepage": "https://github.com/solutosoft/yii-audit-record",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Leandro Guindani Gehlen",
      "email": "leandrogehlen@gmail.com",
      "homepage": "https://github.com/leandrogehlen"
    }
  ],
  "support": {
    "issues": "https://github.com/solutosoft/yii-audit-record/issues?state=open",
    "source": "https://github.com/solutosoft/yii-audit-record"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.14",
    "paulzi/yii2-json-behavior": "~1.0.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "solutosoft\\auditrecord\\": "src",
      "solutosoft\\auditrecord\\tests\\": "tests"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
